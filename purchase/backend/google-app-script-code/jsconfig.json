{
  "compilerOptions": {
    "target": "ES2019",
    "module": "none",
    "lib": ["ES2019"],
    "checkJs": true,
    "noEmit": true,
    "allowJs": true,
    "maxNodeModuleJsDepth": 0,
    "types": ["google-apps-script"],
    "typeRoots": ["./node_modules/@types"]
  },
  "include": [
    "*.js",
    "*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
